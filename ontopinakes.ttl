#    Object Properties
#################################################################

###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#contains
pinakes:contains rdf:type owl:ObjectProperty ;
                 owl:inverseOf pinakes:isContainedIn ;
                 rdfs:label "Contém"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasAddress
pinakes:hasAddress rdf:type owl:ObjectProperty ;
                   owl:inverseOf pinakes:isAddressOf ;
                   rdfs:domain pinakes:Biblioteca ;
                   rdfs:range pinakes:Endereco ;
                   rdfs:comment "Localiza uma Biblioteca a um endereço postal ou coordenadas geográficas"@pt-br ;
                   rdfs:label "Tem endereço"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasAdminNature
pinakes:hasAdminNature rdf:type owl:ObjectProperty ;
                       rdfs:isDefinedBy "Tem Natureza Administrativa"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasAffiliation
pinakes:hasAffiliation rdf:type owl:ObjectProperty ;
                       owl:inverseOf pinakes:isAffiliatedWith ;
                       rdfs:domain pinakes:Biblioteca ;
                       rdfs:range pinakes:Instituicao ;
                       rdfs:comment "Relaciona pelo menos uma biblioteca ou sistema de bibliotecas com obrigatoriamente uma instituição"@pt-br ;
                       rdfs:label "tem filiação"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasCity
pinakes:hasCity rdf:type owl:ObjectProperty ;
                owl:inverseOf pinakes:isCityOf ;
                rdfs:domain pinakes:Pais ;
                rdfs:range pinakes:Localidade ;
                rdfs:comment "Vincula instancias entre países e cidades."@pt-br ;
                rdfs:label "Tem cidade ou localidade"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasCollection
pinakes:hasCollection rdf:type owl:ObjectProperty ;
                      owl:inverseOf pinakes:isCollectionOf ;
                      rdfs:domain pinakes:Biblioteca ;
                      rdfs:range pinakes:Colecao ;
                      rdfs:comment "Relacionamento entre coleções e Bibliotecas" ;
                      rdfs:isDefinedBy "Propriedade que vincula uma bibliotecas á coleções que ela disponibiliza." ;
                      rdfs:label "Tem Coleção"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasControlledSubject
pinakes:hasControlledSubject rdf:type owl:ObjectProperty ;
                             owl:inverseOf pinakes:isControlledSubjectOf ;
                             rdfs:domain pinakes:PublicacaoSeriada ;
                             rdfs:range pinakes:AssuntoControlado ;
                             rdfs:comment "A conexão lógica entre uma Publicação Seriada e uma entidade de assunto controlado relacionada serve como base tanto para identificar o assunto de uma Publicação Seriada individual como para garantir que todos as Publicações Seriadas relevantes para um determinado assunto estejam vinculadas a ele."@pt-br ;
                             rdfs:isDefinedBy "Este relacionamento vincula obrigatoriamente Publicação(ões) seriada(s) ao(s) assunto(s)."@pt-br ;
                             rdfs:label "Possui assunto controlado"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasCountry
pinakes:hasCountry rdf:type owl:ObjectProperty ;
                   owl:inverseOf pinakes:isCountryOf ;
                   rdfs:domain pinakes:UnidadeFederativa ;
                   rdfs:range pinakes:Pais ;
                   rdfs:label "Tem país"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasCountryOfPublication
pinakes:hasCountryOfPublication rdf:type owl:ObjectProperty ;
                                owl:inverseOf pinakes:isCountryPublicationOf ;
                                rdfs:domain pinakes:PublicacaoSeriada ;
                                rdfs:range pinakes:Pais ;
                                rdfs:comment "Relaciona cada publicação seriada a ao país onde foi publicada." ;
                                rdfs:isDefinedBy "Propriedade que vincula uma publicação seriada ao País de publicação" ;
                                rdfs:label "Tem país de Publicação"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasFederativeUnit
pinakes:hasFederativeUnit rdf:type owl:ObjectProperty ;
                          owl:inverseOf pinakes:isFederativeUnitOf ;
                          rdfs:label "Tem unidade federativa"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasFreeTerm
pinakes:hasFreeTerm rdf:type owl:ObjectProperty ;
                    owl:inverseOf pinakes:isFreeTerm ;
                    rdfs:label "Tem termo livre"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasHomePage
pinakes:hasHomePage rdf:type owl:ObjectProperty ;
                    owl:inverseOf pinakes:isHomePageOf ;
                    rdfs:domain pinakes:PublicacaoSeriada ;
                    rdfs:range pinakes:HomePage ;
                    rdfs:comment "Relaciona cada Publicacao seriada a endereço(s) eletrônico(s)"@pt-br ;

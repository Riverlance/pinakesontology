#    Data properties
#################################################################

###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#CCN
pinakes:CCN rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf pinakes:servicos ;
            rdfs:domain pinakes:Biblioteca ;
            rdfs:range rdfs:Literal ;
            rdfs:comment "Catálogo de acesso público que reúne informações sobre as coleções de publicações seriadas nacionais e estrangeiras disponíveis nas bibliotecas brasileiras"@pt-br ;
            rdfs:label "O Catálogo Coletivo Nacional de Publicações Seriadas (CCN),"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#COMUT
pinakes:COMUT rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf pinakes:servicos ;
              rdfs:domain pinakes:Biblioteca ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "Serviço de solicitação de fotocópias e/ou empréstimo de documentos em bibliotecas nacionais e/ou estrangeiras."@pt-br ;
              rdfs:label "Programa de Comutação Bibliográfica (Comut)"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#alfabetacao
pinakes:alfabetacao rdf:type owl:DatatypeProperty ;
                    rdfs:domain pinakes:PublicacaoSeriada ;
                    rdfs:range rdfs:Literal ;
                    rdfs:label "Alfabetação"@pt-br .



                          rdfs:domain pinakes:PublicacaoSeriada ;
                          rdfs:range pinakes:AreaDeConhecimento ;
                          rdfs:label "Tem área de conhecimento"@pt-br .


###  http://www.semanticweb.org/kely/ontologies/2023/11/pinakes#hasLibraryType
pinakes:hasLibraryType rdf:type owl:ObjectProperty ;
                       owl:inverseOf pinakes:libraryTypeOf ;
